{
    "manifest_version": 3,
    "name": "検索履歴推論",
    "description": "検索履歴を推論エンドポイントに送信",
    "version": "1.0",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab"
    ]
  }